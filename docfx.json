{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "ragemp-dotnet-core/src/AlternateLife.RageMP.Net/AlternateLife.RageMP.Net.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "api/**.yml",
          "api/index.md",
          "documentation/toc.yml",
          "documentation/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/alternatelife"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  },
  "pdf": {
    "content": [
      {
        "files": [
            "api/**.yml",
            "api/index.md",
            "documentation/toc.yml",
            "documentation/**.md"
        ],
        "exclude": [
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": [
          "api/toc.yml",
          "api/index.md",
          "toc.yml",
          "*.md",
          "pdf/*"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "out_pdf"
  }
}